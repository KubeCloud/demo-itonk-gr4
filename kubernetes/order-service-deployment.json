{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "order-service",
    "labels": {
      "run": "order-service",
      "visualize": "true"
    }
  },
  "spec": {
    "replicas": 2,
    "template": {
      "metadata": {
        "labels": {
          "app": "order-service",
          "tier": "backend",
          "track": "stable",
          "run": "order-service",
          "visualize": "true"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "order-service",
            "image": "sebastiansondergaard/itonk-order-service"
          }
        ]
      }
    }
  }
}
