{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "config-server",
    "labels": {
      "run": "config-server",
      "visualize": "true"
    }
  },
  "spec": {
    "replicas": 3,
    "template": {
      "metadata": {
        "labels": {
          "app": "config-server",
          "tier": "backend",
          "track": "stable",
          "run": "config-server",
          "visualize": "true"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "config-server",
            "image": "sebastiansondergaard/itonk-config-server"
          }
        ]
      }
    }
  }
}
